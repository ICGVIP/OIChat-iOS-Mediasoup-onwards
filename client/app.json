{
  "expo": {
    "name": "OIChat",
    "slug": "OIChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "oichat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.oichat.ios",
      "buildNumber": "55"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/splash.png",
        "backgroundImage": "./assets/splash.png",
        "monochromeImage": "./assets/splash.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "app.oichat.ios"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "expo-video",
      "expo-audio",
      "expo-asset"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extras": {
      "eas": {
        "projectId": "c8428c34-b5d1-4803-98bf-685d531b8b07"
      }
    }
  }
}
